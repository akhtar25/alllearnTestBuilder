{
  "addons": [
    "papertrail",
    "heroku-postgresql",
    "searchbox"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://buildpack-registry.s3.amazonaws.com/buildpacks/heroku-community/apt.tgz"
    }
  ],
  "env": {
    "AWS_ACCESS_KEY_ID": {
      "required": true
    },
    "AWS_SECRET_ACCESS_KEY": {
      "required": true
    },
    "ELASTICSEARCH_URL": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "S3_BUCKET_NAME": {
      "required": true
    },
    "SEARCHBOX_SSL_URL": {
      "required": true
    },
    "SEARCHBOX_URL": {
      "required": true
    },
    "DATABASE_URL": {
      "required": true
    },
	"EMAIL_API_KEY": {
      "required": true
    },
    "GOOGLE_CLIENT_ID": {
        "required": true
      },
      "GOOGLE_CLIENT_SECRET": {
          "required": true
        }
